<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="logic.Decision" table="decision">
        <id column ="idDecision" name="id" type="java.lang.Integer">
            <generator class="increment"/>
        </id>
        <property column="Date" name="date" type="java.util.Date"/>
        <property column="Comment" name="comment" type="java.lang.String"/>

        <!--Вставить ПРОФІЛЬ!!!!!!!-->
        <property column="specialist_idspecialist" name="specialistId" type="java.lang.Integer"/>

    </class>
</hibernate-mapping>